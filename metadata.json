{
  "name":         "opentable-remaster",
  "version":      "0.1.2",
  "author":       "opentable",
  "license":      "MIT",
  "summary":      "Module for managing the process of moving agent nodes to point to a new master",
  "source":       "https://github.com/opentable/puppet-remaster",
  "project_page": "https://github.com/opentable/puppet-remaster",
  "issues_url":   "https://github.com/opentable/puppet-remaster/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["5.0","6.0"]
    },
    { "operatingsystem": "CentOS",
      "operatingsystemrelease": ["5.0","6.0"]
    },
    { "operatingsystem": "Debian",
      "operatingsystemrelease": ["6.0","7.0"]
    },
    { "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04","14.04"]
    },
    { "operatingsystem": "windows",
      "operatingsystemrelease": ["6.1.7600","6.1.7601", "6.2.9200", "6.3.9200", "6.3.9600"]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.0.0 <5.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.0.0 <2.0.0"
    },
    {
      "name": "opentable/altlib",
      "version_requirement": ">= 0.2.0 <1.0.0"
    }
  ]
}
